<?xml version="1.0" encoding="utf-8"?>
<?qbxml version="13.0"?>
<QBXML>
  <QBXMLMsgsRq onError="stopOnError">
    <DepositAddRq>
      <DepositAdd>
        <!-- required -->
        <TxnDate >{{Date}}</TxnDate>
        <!-- optional -->
        <DepositToAccountRef>
          <!-- required -->
          <FullName >{{DepositToAccount}}</FullName> 
        </DepositToAccountRef>
        <Memo>{{Memo}}</Memo> 
        {{DepositLines}}
      </DepositAdd>
    </DepositAddRq>
  </QBXMLMsgsRq>
</QBXML>